        "eslint-plugin-jsx-a11y": "^6.4.1",
        "@mui/icons-material": "^5.0.4",
        "@mui/lab": "^5.0.0-alpha.51",
        "@mui/material": "^5.0.4",
        "@mui/styles": "^5.0.1",
        "eslint-plugin-prettier": "^3.4.0",
