        "react-router-dom": "^5.2.0",
        "react-scripts": "^4.0.3",
        "react-swipeable-views": "^0.14.0",
        "react-thunk": "^1.0.0",
        "recharts": "^2.1.8",
        "redux": "^4.1.0",
        "redux-debounced": "^0.5.0",
        "redux-thunk": "^2.3.0",
        "web3": "^1.3.6",
        "xlsx": "^0.18.0"
